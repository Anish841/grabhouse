{
    "name": "Grab House",
    "version": "0.0.1",
    "manifest_version": 2,
    "browser_action": {
        "default_title": "Grab House",
        "default_popup": "popup.html"
    },
	"content_scripts": [{
        "matches": ["https://grabhouse.com/seeker/*"],
        "js": ["content.js"]
    }]
}